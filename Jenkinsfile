            steps {
                echo "build"
            }
        }
        stage("test") {
            steps {
                echo "test"
            }
        }
        stage("validate") {
            steps {
                echo "validate"
            }
        }
        stage("deploy") {
            steps {
                echo "deploy"
            }
        }
    }
}
